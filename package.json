{
  "name": "backend",
  "version": "1.0.0",
  "description": "just furniture backend",
  "dependencies": {
    "axios": "^0.21.1",
    "class-transformer": "0.2.3",
    "class-transformer-validator": "0.9.1",
    "class-validator": "0.13.1",
    "convict": "6.0.0",
    "dotenv": "8.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ethers": "^5.6.9",
    "express": "4.17.1",
    "lodash": "^4.17.21",
    "moment": "2.26.0",
    "nodemon": "^2.0.19",
    "pg": "8.5.1",
    "reflect-metadata": "0.1.13",
    "routing-controllers": "0.8.1",
    "sql-formatter": "2.3.3",
    "typeorm": "0.2.19",
    "web3": "^1.7.5",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/convict": "4.2.1",
    "@types/express": "4.17.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "13.1.6",
    "@types/sql-formatter": "2.3.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "cross-env": "7.0.3",
    "eslint": "^8.13.0",
    "prettier": "2.6.2",
    "ts-node": "3.3.0",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.19.0",
    "tspath_ext": "1.3.15",
    "typedi": "0.8.0",
    "typescript": "3.8.3"
  },
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/app.ts",
    "build": "rm -rf build && tsc && node tsconfig-paths-bootstrap.js",
    "lint:fix": "eslint . --ext .ts . --fix",
    "migration:generate": "npm run build && npx typeorm migration:generate -n",
    "migration:create": "npx typeorm migration:create -n",
    "migration:run": "npx typeorm migration:run",
    "migration:revert": "npx typeorm migration:revert",
    "seed:create": "npx typeorm migration:create -c seeds -n"
  }
}
